#!/bin/bash
# svn-to-git
# Created Mon Jun 13 14:47:05 AKDT 2016
# Copyright (C) 2016 by Raymond E. Marcil <marcilr@gmail.com>
#
# Converted svn repository to git.
#

# =================================================
# Configuration
# =================================================
TMPDIR=/tmp/${0}

SVNROOT=/data/svnroot
GITROOT=/data/git

# =================================================
# Binaries
# =================================================
BASENAME=/usr/bin/basename



# =================================================
# Main
# =================================================

BASE=`${BASENAME} "${0}"`

TMPDIR="/tmp/${BASE}"

echo "TMPDIR: ${TMPDIR}"
