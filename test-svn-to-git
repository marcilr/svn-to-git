#
# Test driver for svn-to-git svn to git script
#

# =================================================
# Configuration
# =================================================
SVNREPO=/data/svnroot/tex/tex
GITREPO=/tmp/tex


# =================================================
# Binaries
# =================================================
SVNTOGIT=./svn-to-git


CMD="${SVNTOGIT} -d ${SVNREPO} ${GITREPO}"
echo "Executing: ${CMD}"
eval "${CMD}"
